import java.util.Scanner;

class HelloWorld {
    public static void AVG() {
        Scanner sc = new Scanner(System.in);
        int sum = 0;
        for (int i = 0; i < 5; i++)
        {
            System.out.println("Enter a grade");
            sum += sc.nextInt();
        }
        System.out.println(sum/5);
    }
    public static void main(String[] args) {
        AVG();
    }
}
