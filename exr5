import java.util.Scanner;

class HelloWorld {
    public static void ex5(int num) {
        for (int i = 1; i < num; i++)
        {
            if (num%i == 0)
            {
                System.out.print(i + ", ");
            }
        }
        System.out.println(num);
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter a number");
        int num = sc.nextInt();
        ex5(num);
    }
}
